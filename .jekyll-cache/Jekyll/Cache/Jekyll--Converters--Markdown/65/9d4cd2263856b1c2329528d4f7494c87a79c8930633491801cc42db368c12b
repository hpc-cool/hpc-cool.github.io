  <span class="c1">/// public ActionResult Login(string returnUrl) {</span>
  <span class="c1">///   ViewBag.ReturnUrl = Url.Action("Foo"); // OK</span>
  <span class="c1">///   return RedirectToAction("Bar"); // Error: Cannot resolve action</span>
  <span class="c1">/// }</span>
  <span class="c1">/// &lt;/code&gt;&lt;/example&gt;</span>
  <span class="p">[</span><span class="nf">AttributeUsage</span><span class="p">(</span><span class="n">AttributeTargets</span><span class="p">.</span><span class="n">Parameter</span> <span class="p">|</span> <span class="n">AttributeTargets</span><span class="p">.</span><span class="n">Property</span><span class="p">)]</span>
