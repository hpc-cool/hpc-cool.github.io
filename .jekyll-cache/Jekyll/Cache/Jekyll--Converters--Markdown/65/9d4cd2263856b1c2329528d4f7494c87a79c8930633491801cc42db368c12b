  <span class="p">[</span><span class="nf">AttributeUsage</span><span class="p">(</span><span class="n">AttributeTargets</span><span class="p">.</span><span class="n">Parameter</span><span class="p">)]</span>
  <span class="k">public</span> <span class="k">sealed</span> <span class="k">class</span> <span class="nc">NoEnumerationAttribute</span> <span class="p">:</span> <span class="n">Attribute</span> <span class="p">{</span> <span class="p">}</span>

