        <span class="p">{</span>
            <span class="k">public</span> <span class="nf">UnicodeRangeInfo</span><span class="p">(</span><span class="kt">string</span> <span class="n">unicodeRangeName</span><span class="p">,</span> <span class="n">UnicodeRange</span> <span class="n">unicodeRange</span><span class="p">)</span>
