        <span class="k">private</span> <span class="kt">bool</span> <span class="n">_isSignaled</span><span class="p">;</span>

        <span class="k">private</span> <span class="k">readonly</span> <span class="kt">object</span> <span class="n">_locker</span> <span class="p">=</span> <span class="k">new</span> <span class="kt">object</span><span class="p">();</span>
    <span class="p">}</span>
